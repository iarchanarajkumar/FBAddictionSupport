{
	"browser_action": {
        "default_icon": "eye.png",
        "default_popup": "popup.html"
    },
  "name": "FbAnti",
  "version": "1.0",
  "description": "Alerts you if you sit on certain networking sites for more than 2 mins",
  "manifest_version": 2,
   "permissions": [
        "tabs",
        "https://www.facebook.com/*"
      
    ],
     "content_scripts": [
        {
           "css": [
                
            ],
            "js": [
                "tracker.js"/*,
            ,*/
            ],
            "matches": [
                
                 "https://www.facebook.com/*"
                       
                ],
            "run_at": "document_end"
        }
    ]

 
}
